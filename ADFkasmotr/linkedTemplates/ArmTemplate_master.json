{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFkasmotr"
		},
		"LK_ABS_Input_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LK_ABS_Input'"
		},
		"LK_AzureSqlDatabase1_output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LK_AzureSqlDatabase1_output'"
		},
		"LK_Sink _OUTPUT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LK_Sink _OUTPUT'"
		},
		"TR_Storage_Event_test1_properties_TestEventTrigger_parameters_fileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"TR_Storage_Event_test1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/757ba34a-2ccf-4eb1-85da-b102d4875ef7/resourceGroups/RG_ADF_KASMO_TR/providers/Microsoft.Storage/storageAccounts/storageaccountkasmo1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFkasmotr_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LK_ABS_Input_connectionString": {
						"value": "[parameters('LK_ABS_Input_connectionString')]"
					},
					"LK_AzureSqlDatabase1_output_connectionString": {
						"value": "[parameters('LK_AzureSqlDatabase1_output_connectionString')]"
					},
					"LK_Sink _OUTPUT_connectionString": {
						"value": "[parameters('LK_Sink _OUTPUT_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFkasmotr_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TR_Storage_Event_test1_properties_TestEventTrigger_parameters_fileName": {
						"value": "[parameters('TR_Storage_Event_test1_properties_TestEventTrigger_parameters_fileName')]"
					},
					"TR_Storage_Event_test1_properties_typeProperties_scope": {
						"value": "[parameters('TR_Storage_Event_test1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFkasmotr_ArmTemplate_0')]"
			]
		}
	]
}